<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
<Document>
  <name>RACE GIS</name>
  <Style id="project-style">
  <LabelStyle>
   <scale>0</scale>
  </LabelStyle>
   <IconStyle>
    <scale>1</scale>
    <Icon>
      <href>http://maps.google.com/mapfiles/kml/paddle/blu-blank.png</href>
    </Icon>
    </IconStyle>
    <BalloonStyle>
      <text>
        <![CDATA[
	  <h2>$[name]</h2>
          <p><b>ID: </b>$[ID]</p>
	  <p><b>Address: </b>$[Address]</p>
          <p><b>Project Type: </b>$[projectType]</p>
          <p><b>Client Name: </b>$[clientName]</p>
          <p><b>Client Type: </b>$[clientType]</p>
          <p><b>Permit: </b> $[Permit]</p>
          <p><b>Boring: </b> $[Boring]</p>
        ]]>
      </text>
    </BalloonStyle>
  </Style>
  <!-- Shared style sample
        Two Placemarks use the same balloon template
  -->
  <Placemark>
    <name>House Dredge</name>
    <styleUrl>#project-style</styleUrl>
    <ExtendedData>
      <Data name="ID">
	<value>2019001</value>
      </Data>
      <Data name="Address">
        <value>858 Riverton Terrace Stratford CT</value>
      </Data>
      <Data name="projectType">
        <value>Dredging</value>
      </Data>
      <Data name="clientType">
        <value>Residential</value>
      </Data>
      <Data name="clientName">
        <value>Stephen Duncanson</value>
      </Data>
      <Data name="Permit">
        <value>NO</value>
      </Data>
      <Data name="Boring">
        <value>YES</value>
      </Data>
    </ExtendedData>
    <Point>
         <coordinates>-73.1216929999683,41.2166794166201,0</coordinates>    
    </Point>
  </Placemark>
  </Document>
</kml>